{
  "name": "TutorConnect - Norges ledende plattform for privatlæring",
  "short_name": "TutorConnect",
  "description": "Finn kvalifiserte lærere og studenter for privatundervisning i Norge. Trygge, verifiserte profiler og enkel booking.",
  "start_url": "/",
  "scope": "/",
  "display": "standalone",
  "display_override": ["window-controls-overlay", "standalone", "minimal-ui"],
  "orientation": "portrait-primary",
  "theme_color": "#0ea5e9",
  "background_color": "#ffffff",
  "categories": ["education", "productivity", "social"],
  "lang": "no",
  "dir": "ltr",
  "icons": [
    {
      "src": "/icons/icon-72x72.png",
      "sizes": "72x72",
      "type": "image/png",
      "purpose": "maskable any"
